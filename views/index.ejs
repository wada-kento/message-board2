<!DOCTYPE html>
<html>

<head>
    <title>
        メッセージボード
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <h1>メッセージボード</h1>
    <ul>
        <% messages.map(function(message) { %>
            <li>
                <p>
                    <%= message.content %>
                </p>
            </li>
            <% }); %>
    </ul>
</body>

</html>